{
    "compilerOptions": {
      "module": "commonjs",
      "declaration": true,
      "target": "es6",
      "sourceMap": true,
      "noImplicitAny": false,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "noImplicitThis": false,
      "outDir": "./dist",
      "moduleResolution": "node"
    },
    "compileOnSave": true,
    "exclude": [
      "node_modules",
      "dist"
    ]
  }
  
  